//div.settings-wrapper(data-bind="visible: visible")
div.control-table(data-old-bind='foreach: settings')
    div.control-item(data-old-bind='css: {"active": value}')
        div.control-label
            span.icon(data-old-bind='css: iconClass')
        div.control-label
            label(data-old-bind='attr: {"for": id, title: promptOrTitle}, text: name', for='use-stream-api')
        div.control-element
            input(data-old-bind='attr: {"id": id, "type": type, placeholder: promptOrTitle, list: suggestListId}, value: realValue, checked: checked, valueUpdate: "input"', type='text')
            datalist(data-old-bind='attr: {"id": suggestListId}, foreach: suggestList')
                option(data-old-bind='attr: {"value": $data}')

